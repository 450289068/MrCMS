@using MrCMS.Helpers
@model IList<Type>
@{
    ViewBag.Title = "Message Templates";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";

}
<form>
    <fieldset>
        <legend>Message Templates
        </legend>
        
        <table class="table table-condensed table-striped">
            <tr>
                <th>Name
                </th>
                <th>
                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.GetFriendlyName()
                    </td>
                    <td>
                        asdf
                    </td>
                </tr>
            }
        </table>
    </fieldset>
</form>
