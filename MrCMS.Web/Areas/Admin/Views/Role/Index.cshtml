@using MrCMS.Entities.People
@model System.Collections.Generic.IEnumerable<MrCMS.Entities.People.UserRole>
@{
    ViewBag.Title = "Roles";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";

}

<h2>
    User Roles</h2>
@Html.ActionLink("Add", "Add", null, new {@class = "btn btn-success", data_toggle = "modal"})
<br />
<br />
<table class="table table-striped">
    <tr>
        <th>
            Name
        </th>
        <td>
            
        </td>
    </tr>
    @foreach (UserRole role in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(role.Name, "Edit", new { id = role.Id }, new { @class = "", data_toggle = "modal" })
            </td>
            <td>@Html.ActionLink("Delete", "Delete", new { id = role.Id }, new {@class = "btn btn-danger", data_toggle = "modal"})</td>
        </tr>
    }
</table>